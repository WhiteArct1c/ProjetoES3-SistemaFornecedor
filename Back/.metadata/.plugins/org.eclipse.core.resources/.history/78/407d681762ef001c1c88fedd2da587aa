package controller.web;

import jakarta.servlet.http.HttpServletRequest;
import jakarta.servlet.http.HttpServletResponse;
import model.EntidadeDominio;
import model.Servico;

public class ServicoVH implements IViewHelper {

	@Override
	public EntidadeDominio getEntidade(HttpServletRequest request) {
		
		Servico servico = new Servico(request.getParameter("nomeServico"), 
				request.getParameter("descricaoServico"), Double.parseDouble(request.getParameter("precoServico")));
		
		return servico;
	}

	@Override
	public void setEntidade(HttpServletResponse response, HttpServletRequest request, Object msg) {
		

	}

}
